<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.ac.ulb.infof307.g06.presentation.versioning.createbranch.CreateBranchView">
    <center>
        <VBox alignment="CENTER" prefHeight="150.0" prefWidth="369.0" spacing="10.0">
            <children>
                <Label id="page-title" alignment="CENTER" text="Create Branch"></Label>
                <Label text="Branch name" />
                <TextField fx:id="branchNameTextField" promptText="Branch name" />
                <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#confirmButtonClicked" prefHeight="25.0" prefWidth="80.0" text="Confirm"> </Button>
            </children>
        </VBox>
    </center>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</fx:root>
